<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">



	<Button android:id="@+id/musicService" 
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:text="@string/musicService" />
		
	<Button android:id="@+id/bindMusicService" 
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:text="@string/bindMusicService" />

	<Button android:id="@+id/remoteMusicService" 
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:text="@string/remoteMusicService" />
	
	<Button android:id="@+id/MusicReceiver" 
		android:layout_width="fill_parent"
		android:layout_height="wrap_content" 
		android:text="Music Receiver" />
	
</LinearLayout>
